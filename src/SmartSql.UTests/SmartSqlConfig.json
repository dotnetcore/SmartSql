{
  "Settings": {
    "IgnoreParameterCase": "false",
    "IsCacheEnabled": "false",
    "IsWatchConfigFile": "false",
    "ParameterPrefix": "$"
  },
  "Database": {
    "DbProvider": {
      "Name": "SqlClientFactory",
      "ParameterPrefix": "@",
      "Type": "System.Data.SqlClient.SqlClientFactory,System.Data.SqlClient"
    },
    "Write": {
      "Name": "WriteDB",
      "ConnectionString": "Data Source=.;Initial Catalog=SmartSqlStarterDB;Integrated Security=True"
    }
  },
  "SmartSqlMaps": [
    {
      "Path": "Maps",
      "Type": "Directory"
    }
  ],
  "TypeHandlers": [
    {
      "Name": "Json",
      "Type": "SmartSql.TypeHandler.JsonTypeHandler,SmartSql.TypeHandler"
    }
  ]
}